// deleting property from an object
const person={name:'Ritik', 
age:23, 
city:'Seoni malwa',
college:'svce',
address:{city:'Seoni malwa',
pincode:'461223',
state:'MP'
}
}
console.log(person);
result= delete person.age;
delete person.address.city;
console.log(result);
console.log(person);

